    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.31",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^7.2.0",
    "faker": "^5.5.2",
    "husky": "^3.1.0",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
